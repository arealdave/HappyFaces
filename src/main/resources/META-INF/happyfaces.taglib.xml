<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE facelet-taglib PUBLIC
"-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
"http://java.sun.com/dtd/facelet-taglib_1_0.dtd">

<facelet-taglib>
	<namespace>http://happyfaces.github.com/hf</namespace>
	<tag>
		<tag-name>calendar</tag-name>
		<component>
			<component-type>CalendarComponentFamily</component-type>
			<renderer-type>CalendarRenderer</renderer-type>
		</component>
	</tag>
	<tag>
		<tag-name>helloworld</tag-name>
		<component>
			<component-type>HelloWorldComponentFamily</component-type>
			<renderer-type>HelloWorldRenderer</renderer-type>
		</component>
	</tag>
</facelet-taglib>